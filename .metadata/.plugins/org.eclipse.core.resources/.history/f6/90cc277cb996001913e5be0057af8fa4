package br.edu.ifpe.igarassu.ipi.poo;

public class Entity {
	
	private long id;
	
	public Entity(long id) {
		super();
		this.setId(id);
	}

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}
	
	
}
