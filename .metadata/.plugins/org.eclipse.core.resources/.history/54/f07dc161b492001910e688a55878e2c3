package br.edu.ifpe.igarassu.ipi.poo.usuario;

import br.edu.ifpe.igarassu.ipi.poo.Entidade;

public class User extends Entidade {
	private String nome;
	private String senha;
	
	public User(long id, String nome, String senha) {
		super(id);
		this.nome = nome;
		this.senha = senha;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public String getSenha() {
		return senha;
	}

	public void setSenha(String senha) {
		this.senha = senha;
	}

}
